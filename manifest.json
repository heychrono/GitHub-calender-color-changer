{
  "manifest_version": 2,
  "name": "GitHub contribution theme",
  "description": "Change color of contribution on your GitHub Profile",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "GitHub contribution theme"
  }

}